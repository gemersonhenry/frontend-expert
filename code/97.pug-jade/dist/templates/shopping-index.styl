@import "organisms/product-list/product-list.styl"
@import "organisms/special-list/special-list.styl"
$max-width = 1080px

.content-shopping
  max-width $max-width
  margin 0 auto
  overflow hidden
  .content-center
    width 75%
    float left
    product-list(100%)
  .content-right
    width 25%
    float left
    padding-left 10px
    box-sizing border-box
    product-special(100%)
